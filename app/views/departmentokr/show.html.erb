<div class="tabs">
	<div id="tab-1" class="tab">
		<article>
			<div class="text-section">
				<h1><p id="notice"><%= @notice %></p></h1>
			</div>
			
		<ul class="states" >
			<h1>OKR详细信息</h1>
				<p>
				  <strong>所属部门名称:</strong>
				  <%= @department_okr.department_id %>
				</p>
				<p>
				  <strong>OKR内容:</strong>
				  <%= @department_okr.okr_name %>
				</p>
				<p>
				  <strong>OKR考核月份:</strong>
				  <%= @department_okr.okr_date %>
				</p>
				<p>
				  <strong>OKR占比:</strong>
				  <%= @department_okr.okr_proportion %>
				</p>
				<p>
				  <strong>OKR完成状态:</strong>
				  <%= @department_okr.okr_stats %>
				</p>
				<p>
				  <strong>OKR完成得分:</strong>
				  <%= @department_okr.okr_score %>
				</p>
				<p>
				  <strong>OKR得分评定人:</strong>
				  <%= @department_okr.assessment_person %>
				</p>
				<p>
				  <strong>OKR得分评定时间:</strong>
				  <%= @department_okr.assessment_time %>
				</p>
				<p>
				  <strong>创建时间:</strong>
				  <%= @department_okr.created_at.strftime("%Y-%m-%d %H:%M:%S") %>
				</p>
				<p>
				  <strong>更新时间:</strong>
				  <%= @department_okr.updated_at.strftime("%Y-%m-%d %H:%M:%S") %>
				</p>
				<%= link_to 'Edit', edit_departmentokr_path(@department_okr) %> |
				<%= link_to 'Back', '/departmentokr' %> | 
			</ul>

		</article>
	</div>
</div>